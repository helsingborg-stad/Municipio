{
  "name": "Municipio",
  "version": "3.0.0",
  "description": "The official website of Helsingborg Stad",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-imports": "webpack --config webpack.import.js",
    "build": "webpack --config webpack.config.js & node-sass --recursive --output-style compressed node_modules/@helsingborg-stad/source/sass/main.scss assets/dist/3.0/css/styleguide-css.min.css",
    "start": "webpack-dev-server --inline --hot --config webpack.dev.config.js",
    "sass": "node-sass --recursive --watch --output-style compressed source/sass/main.scss assets/dist/3.0/css/styleguide-css.min.css",
    "lint": "stylelint \"./source/sass/**/*.scss\" --output-file stylelint.log && eslint -c .eslintrc.js source/js",
    "lint-autofix": "stylelint \"./source/sass\" --fix && eslint -c .eslintrc.js source/js --fix",
    "express-server": "node build/expressServer.js",
    "phpcs-report": "php phpcs.phar source/library --standard=psr2",
    "phpcs-fix": "php phpcbf.phar source/library --standard=psr2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helsingborg-stad/styleguide.git"
  },
  "files": [
    "assets/js/*",
    "source/sass/*",
    "build/*",
    "assets/*",
    "webpack.import.js",
    "webpack.config.js",
    "webpack.dev.config.js",
    "LICENSE"
  ],
  "keywords": [],
  "author": "HBG Works, Helsingborg Stad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helsingborg-stad/styleguide/issues"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "homepage": "https://github.com/helsingborg-stad/styleguide#readme",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.4",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "eslint-config-hbg": "^0.1.1",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "filemanager-webpack-plugin": "^2.0.5",
    "fs": "0.0.1-security",
    "hbg-mix": "^2.1.2",
    "material-design-icons": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass": "^1.23.7",
    "sass-loader": "^7.3.1",
    "stylelint": "^12.0.0",
    "stylelint-webpack-plugin": "^1.1.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-fix-style-only-entries": "^0.2.2",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "clientside-require": "^4.7.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "jquery": "^3.4.1",
    "masonry-layout": "^4.2.2",
    "restify": "^8.5.1",
    "restify-router": "^0.5.1",
    "webpack-dev-server": "^3.9.0"
  },
  "browser": {
    "child_process": false
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
