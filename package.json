        "@material-symbols/font-600": "^0.27.0",
        "@types/jest": "^29.5.14",
        "babel-loader": "^8.2.3",
        "body-parser": "^1.19.1",
        "browser-sync": "^3.0.4",
        "browser-sync-webpack-plugin": "^2.3.0",
        "clean-webpack-plugin": "^4.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.6.0",
        "css-minimizer-webpack-plugin": "^3.4.1",
